   "source": [
    "def generate_seq(start, end):\n",
    "    if start > end:\n",
    "        return []  \n",
    "\n",
    "    seq = []\n",
    "    curr = start\n",
    "\n",
    "    while curr <= end:\n",
    "        seq.append(curr)\n",
    "        curr += 2\n",
    "\n",
    "    return seq\n",
    "\n",
    "start = 0\n",
    "end = 10\n",
    "r = generate_seq(start, end)\n",
    "print(r)"
   ]
